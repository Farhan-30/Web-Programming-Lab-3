<%- include('header'); -%>
    <form method="post" action="/api/register">
        <label>Society</label> <br>
        <input type="text" name="Society" placeholder="Enter your society name:"> <br>
        <br>
        <label>Contact</label> <br>
        <input type="text" name="Contact" placeholder="Enter your contact name:"> <br>
        <br>
        <label>Adress</label> <br>
        <input type="text" name="Adress" placeholder="Enter your adress:"> <br>
        <br>
        <label>Zip Code</label> <br>
        <input type="text" name="Zip Code" placeholder="Enter your zip code:"> <br>
        <br>
        <label>City</label> <br>
        <input type="text" name="City" placeholder="Enter your city name:"> <br>
        <br>
        <label>Phone</label> <br>
        <input type="text" name="Phone" placeholder="Enter your phone number:"> <br>
        <br>
        <label>Fax</label> <br>
        <input type="text" name="Fax" placeholder="Enter your fax number:"> <br>
        <br>
        <label>Confirmation</label> <br>
        <input type="number" name="Confirmation" placeholder="Enter your Confirmation number:"> <br>
        <br>
        <label>Payment</label> <br>
        <input type="text" name="Payment" placeholder="Enter your Payment details:"> <br>
        <br>
        <label>Information</label> <br>
        <input type="text" name="Information" placeholder="Enter your Information details:"> <br>
        <br>
        <label>Max Outstanding</label> <br>
        <input type="number" name="Max Outstanding" placeholder="Enter your Max Outstanding fee:"> <br>
        <br>
    </form>
<%- include ('footer'); -&>